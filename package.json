  "main": "roosevelt.js",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=0.10.0"
  },
  "engineStrict": true,
  "dependencies": {
    "express": "~3.4.6",
    "teddy": "~0.2.32",
    "less": "~1.6.0",
    "formidable": "~1.0.14",
    "wrench": "~1.5.4",
    "colors": "~0.6.2",
    "toobusy": "~0.2.4",
    "update-notifier": "~0.1.7"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": true,
    "devel": true,
    "evil": true,
