{
  "name": "roosevelt",
  "description": "Roosevelt MVC web framework",
  "author": "Eric Newport <kethinov@gmail.com>",
  "version": "0.3.3",
  "homepage": "https://github.com/kethinov/roosevelt",
  "license": "Creative Commons Attribution 3.0 Unported License",
  "main": "roosevelt.js",
  "readmeFilename": "README.md",
  "dependencies": {
    "express": "*",
    "teddy": "*",
    "less-middleware": "*",
    "wrench": "*"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": true,
    "devel": true,
    "evil": true,
    "indent": 2,
    "node": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kethinov/roosevelt.git"
  },
  "keywords": [
    "roosevelt.js",
    "teddy.js",
    "node.js",
    "roosevelt",
    "teddy",
    "mvc"
  ],
  "bin": {
    "roosevelt": "./cli.js"
  },
  "preferGlobal": true,
  "_from": "roosevelt@*",
  "gitHead": "",
  "scripts": {
    "test": "jshint roosevelt.js cli.js sampleApp/app.js sampleApp/mvc/controllers/_404.js sampleApp/mvc/controllers/index.js sampleApp/mvc/models/_404.js sampleApp/mvc/models/index.js"
  }
}
